{
	"name": "joola.io",
	"preferGlobal": false,
	"version": "3.1.1",
	"author": "Joola <info@joo.la>",
	"description": "The open-source data visualization framework",
	"engine": "node >= 0.10.x",
	"private": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/joola/joola.io.git"
	},
	"bugs": "https://joolatech.atlassian.net/browse/JARVIS",
	"contributors": [
		{
			"name": "Itay Weinberger",
			"email": "itay@joo.la",
			"url": "http://joo.la"
		},
		{
			"name": "Or Weinberger",
			"email": "or@joo.la",
			"url": "http://joo.la"
		}
	],
	"scripts": {
		"test": "make test",
		"start": "node ./bin/_joola.io.js",
		"debug": "npm-inspector && npm start && open \"http://http://127.0.0.1:8080/debug?port=5858\"",
		"monitor": "mocha --reporter min --watch",
		"debugtest": "mocha --debug-brk --reporter spec",
		"blanket": {
			"pattern": "lib",
			"data-cover-never": "node_modules"
		},
		"coveralls": "mocha --require blanket --reporter mocha-lcov-reporter | node ./node_modules/coveralls/bin/coveralls.js",
		"citest": "mocha --reporter xunit > test-reports.xml"
	},
	"bin": {
		"joola.io": "./bin/joola.io"
	},
	"keywords": [
		"data",
		"analytics",
		"big-data",
		"visualization",
		"noSQL"
	],
	"dependencies": {
		"joola.io.config": "3.x",
		"joola.io.engine": "3.x",
		"joola.io.analytics": "3.x",

		"winston": "0.x",
		"async": "0.x",
		"path": "0.x",
		"flatiron": "0.x",
		"flatiron-cli-config": "0.x",
		"request": "2.x",
		"dateformat": "1.x",
		"pkginfo": "0.x",
		"colors": "0.x",
		"forever": "0.x",
		"forever-monitor": "1.x",
		"sleep": "1.x",
		"supertest": "0.x",
		"complete": "0.x"
	},
	"devDependencies": {
		"underscore": "1.x",
		"mocha-lcov-reporter": "0.x",
		"coveralls": "2.x",
		"should": "1.x",
		"mocha": "1.x",
		"sinon": "1.x",
		"chai": "1.x",
		"sinon-chai": "2.x",
		"blanket": "1.x",
		"jshint": "2.x",
		"supertest": "0.x"
	},
	"license": "GPL-3.0"
}
