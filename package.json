{
  "name": "joola.io",
  "preferGlobal": false,
  "version": "3.4.0",
  "author": "Joola <info@joo.la>",
  "description": "The open-source data visualization framework",
  "engine": "node >= 0.10.x",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/joola/joola.io.git"
  },
  "bugs": "https://joolatech.atlassian.net/browse/JARVIS",
  "contributors": [
    {
      "name": "Itay Weinberger",
      "email": "itay@joo.la",
      "url": "http://joo.la"
    },
    {
      "name": "Or Weinberger",
      "email": "or@joo.la",
      "url": "http://joo.la"
    }
  ],
  "scripts": {
    "test": "make test",
    "start": "pm2 start service.json",
    "debug": "npm-inspector && npm start && open \"http://http://127.0.0.1:8080/debug?port=5858\"",
    "monitor": "mocha --reporter min --watch",
    "debugtest": "mocha --debug-brk --reporter spec",
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules"
    },
    "coveralls": "make test-coveralls"
  },
  "bin": {
    "joola.io": "./bin/joola.io"
  },
  "keywords": [
    "data",
    "analytics",
    "big-data",
    "visualization",
    "noSQL"
  ],
  "dependencies": {
    "nconf": "*",
    "nconf-redis": "*",
    "eventemitter2": "*",
    "winston": "*",
    "redis": "*",
    "node-uuid": "*",
    "async": "*",
    "express": "*"
  },
  "devDependencies": {
    "socket.io-browserify":"*",
    "underscore": "1.x",
    "mocha-lcov-reporter": "0.x",
    "coveralls": "2.x",
    "should": "1.x",
    "mocha": "1.x",
    "sinon": "1.x",
    "chai": "1.x",
    "sinon-chai": "2.x",
    "blanket": "1.x",
    "jshint": "2.x",
    "supertest": "0.x"
  },
  "license": "GPL-3.0"
}
