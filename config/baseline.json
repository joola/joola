{
  "id": "joola.io-config-baseline",
  "description": "This is a baseline configuration file for joola.io",
  "moreinfo": "https://github.com/joola/joola.io/wiki",

  "_version": "0.0.1",
  "general": {
    "adminemail": "info@joo.la",
    "dateformat": "YYYY-mm-dd hh:ii:ss",
    "systemname": "joola"
  },
  "interfaces": {
    "webserver": {
      "enabled": true,
      "host": "localhost",
      "port": 8080,
      "securePort": 8081,
      "secure": false,
      "keyFile": null,
      "certFile": null,
      "websocket": {
        "enabled": true
      },
      "sessionDurtaion": 1200000,
      "timeout": 15000,
      "blacklist": []
    },
    "repl": {
      "enabled": true,
      "port": "1337"
    }
  },
  "store": {
    "config": {
      "redis": {
        "host": "localhost",
        "port": 6379,
        "db": 0,
        "pass": null
      }
    },
    "dispatch": {
      "redis": {
        "host": "localhost",
        "port": 6379,
        "db": 1,
        "pass": null
      },
      "stomp": {
        "host": "127.0.0.1",
        "port": 61613,
        "user": "guest",
        "pass": "guest"
      }
    },
    "socketio": {
      "redis": {
        "host": "localhost",
        "port": 6379,
        "db": 2,
        "pass": null
      }
    },
    "runtime": {
      "redis": {
        "host": "localhost",
        "port": 6379,
        "db": 3,
        "pass": null
      }
    },
    "logger": {
      "console": {
        "level": "info"
      },
      "mongo": {
        "level": "trace",
        "host": "localhost",
        "port": 27017,
        "user": null,
        "password": null,
        "db": "logger",
        "options": {
          "capped": true,
          "size": 10240000
        },
        "unique": false,
        "expireAfterSeconds": 3600
      },
      "file": {
        "level": "trace",
        "path": "/tmp/joola.io/"
      }
    },
    "cache": {
      "mongo": {
        "host": "localhost",
        "port": 27017,
        "user": null,
        "password": null,
        "db": "cache",
        "expireAfterSeconds": 0
